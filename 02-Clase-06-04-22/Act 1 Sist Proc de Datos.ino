int LedRojo = 13;

  void setup()
{
  pinMode(LedRojo, OUTPUT);
}

void loop()
{
  digitalWrite(LedRojo, HIGH);
  delay(100);
  digitalWrite(LedRojo, LOW);
  delay(100);
  digitalWrite(LedRojo, HIGH);
  delay(200);
  digitalWrite(LedRojo, LOW);
  delay(200);
  digitalWrite(LedRojo, HIGH);
  delay(300);
  digitalWrite(LedRojo, LOW);
  delay(300);
  digitalWrite(LedRojo, HIGH);
  delay(400);
  digitalWrite(LedRojo, LOW);
  delay(400);
  digitalWrite(LedRojo, HIGH);
  delay(500);
  digitalWrite(LedRojo, LOW);
  delay(500);
  digitalWrite(LedRojo, HIGH);
  delay(600);
  digitalWrite(LedRojo, LOW);
  delay(600);
  digitalWrite(LedRojo, HIGH);
  delay(700);
  digitalWrite(LedRojo, LOW);
  delay(700);
  digitalWrite(LedRojo, HIGH);
  delay(800);
  digitalWrite(LedRojo, LOW);
  delay(800);
  digitalWrite(LedRojo, HIGH);
  delay(900);
  digitalWrite(LedRojo, LOW);
  delay(900);
  digitalWrite(LedRojo, HIGH);
  delay(1000);
  digitalWrite(LedRojo, LOW);
  delay(1000);
}